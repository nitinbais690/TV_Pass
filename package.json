{
    "name": "TVPass",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "android": "npx react-native run-android",
        "ios": "npx react-native run-ios",
        "start": "react-native start",
        "test": "jest",
        "test:coverage": "jest --coverage --testResultsProcessor jest-sonar-reporter",
        "test:staged": "./node_modules/.bin/jest --bail --findRelatedTests",
        "start:appium": "cross-os start-appium",
        "stop:appium": "cross-os stop-appium",
        "test:e2e": "jest --testMatch=\"**/__tests__/**/*e2e.js\" --collectCoverage=false",
        "test:e2e:android": "TARGET_PLATFORM=\"android\" INTERFACE_IDIOM=\"mobile\" npm run test:e2e",
        "test:e2e:firetv": "TARGET_PLATFORM=\"android\" INTERFACE_IDIOM=\"tv\" npm run test:e2e",
        "test:e2e:ios": "TARGET_PLATFORM=\"ios\" INTERFACE_IDIOM=\"mobile\"  npm run test:e2e",
        "test:e2e:tvos": "TARGET_PLATFORM=\"tvos\" INTERFACE_IDIOM=\"tv\"  npm run test:e2e",
        "lint": "npx eslint --ext .js,.ts,.tsx src/components",
        "lint:fix": "npx eslint --ext .js,.ts,.tsx src/components --fix",
        "lint:report": "npm run lint -- -f json -o reports/eslint-report.json",
        "pretty:fix": "npx prettier \"src/**/*.ts*\" --write && npx prettier \"src/**/*.js*\" --write",
        "postinstall": "npx jetify && cross-os rn-screens-tvos && patch-package",
        "danger": "npx danger",
        "sonar": "node sonar-project.js"
    },
