        "@types/react-native-elements": "^0.18.0",
        "@types/react-native-indicators": "^0.16.0",
        "@types/react-test-renderer": "16.8.2",
        "@typescript-eslint/eslint-plugin": "^1.12.0",
        "@typescript-eslint/parser": "^1.12.0",
        "babel-jest": "25.1.0",
        "babel-plugin-transform-remove-console": "^6.9.4",
        "cross-os": "^1.3.0",
        "eslint": "^5.16.0",
        "eslint-config-prettier": "^4.3.0",
        "eslint-plugin-eslint-comments": "^3.1.2",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-plugin-react": "^7.24.0",
        "eslint-plugin-react-hooks": "^1.6.1",
        "eslint-plugin-react-native": "^3.11.0",
        "eslint-plugin-sonarjs": "^0.5.0",
        "flipper-plugin-react-native-performance": "^0.5.0",
        "husky": "^2.7.0",
        "jest": "25.1.0",
        "jest-fetch-mock": "^2.1.2",
        "jest-junit": "^6.4.0",
        "jest-sonar-reporter": "^2.0.0",
        "lint-staged": "^8.1.7",
        "metro-react-native-babel-preset": "^0.59.0",
        "patch-package": "^6.4.7",
        "prettier": "^1.19.1",
        "react-native-flipper": "^0.45.0",
        "react-native-svg-transformer": "^0.14.3",
        "react-test-renderer": "16.13.1",
        "sonarqube-scanner": "^2.8.1",
        "ts-jest": "24.0.1",
        "typedoc": "^0.15.0",
        "typescript": "3.4.5",
        "wd": "^1.11.3"
    },
    "jestSonar": {
        "reportPath": "reports",
        "reportFile": "test-report.xml",
        "indent": 4
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "./node_modules/.bin/eslint --fix",
            "git add"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "pre-push": "npm run test -- --silent --lastCommit"
        }
    },
    "cross-os": {
        "rn-screens-tvos": {
            "darwin": "./scripts/misc/rn-screens-tvos.sh"
        },
        "start-appium": {
            "darwin": "appium &"
        },
        "stop-appium": {
            "darwin": "sh scripts/misc/stop_appium.sh"
        }
    }
}
