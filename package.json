        "rn-qp-nxg-player": "http://maven.quickplay.local/nexus/content/repositories/releases/com/quickplay/vstb-quickplaytv/reactnative/rn-qp-nxg-player/1.7.12/rn-qp-nxg-player-1.7.12.tgz",
